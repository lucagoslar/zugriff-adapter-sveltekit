{
	"env": {
		"browser": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:prettier/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 13,
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/ban-ts-comment": "off",
		"no-empty": "off"
	},
	"plugins": ["@typescript-eslint", "prettier"]
}
